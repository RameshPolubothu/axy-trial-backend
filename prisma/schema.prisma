generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model ScientificPaper {
  id          Int      @id @default(autoincrement())
  name        String
  description String
  author      String
  createdAt   DateTime @default(now())
  views       Int      @default(0)
  likes       Int      @default(0)
}